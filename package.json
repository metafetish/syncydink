{
  "name": "syncydink",
  "version": "0.0.2",
  "description": "2D and VR Movie Player with Haptics Integration and Synchronization",
  "author": "Kyle Machulis <kyle@machul.is>",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --open --hot --host 192.168.123.3",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "tslint": "tslint -c tslint.json 'src/**/*.ts' 'tests/**/*.ts'",
    "tslint:fix": "tslint -c tslint.json --fix semicolon,comma,no-var-keyword,ordered-imports 'src/**/*.ts' 'tests/**/*.ts'",
    "webpack-defaults": "webpack-defaults",
    "build:main": "tsc -p tsconfig.json",
    "pretest": "yarn build:main",
    "test": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' mocha --compilers ts:ts-node/register 'tests/**/*.ts'"
  },
  "dependencies": {
    "buttplug": "^0.0.7",
    "haptic-movie-file-reader": "^0.0.6",
    "tslint": "^5.5.0",
    "video.js": "^6.2.1",
    "viewport-units-buggyfill": "^0.6.1",
    "vue": "^2.3.3",
    "vue-awesome": "^2.3.1",
    "vue-material": "^0.7.4",
    "vue-touch": "next",
    "vue-video-player": "^3.1.4"
  },
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.34",
    "@types/video.js": "^5.16.0",
    "chai": "^4.1.0",
    "codecov": "^2.2.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "mocha": "^3.4.2",
    "noop-loader": "^1.0.0",
    "null-loader": "^0.1.1",
    "nyc": "^11.0.3",
    "ts-loader": "^2.1.0",
    "ts-node": "^3.2.0",
    "typescript": "^2.4.1",
    "uglify-es": "^3.0.24",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.1",
    "vue-loader": "^12.1.0",
    "vue-property-decorator": "^5.1.1",
    "vue-template-compiler": "^2.3.3",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.5.1"
  },
  "pre-push": []
}
